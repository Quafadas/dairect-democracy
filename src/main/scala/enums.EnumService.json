{
    "openapi": "3.0.2",
    "info": {
        "title": "EnumService",
        "version": ""
    },
    "paths": {
        "/isFaceCard/{cardValue}/{suit}": {
            "get": {
                "description": "Checks if a card is a face card",
                "operationId": "IsFaceCard",
                "parameters": [
                    {
                        "name": "cardValue",
                        "in": "path",
                        "description": "The face card",
                        "schema": {
                            "type": "integer",
                            "maximum": 13,
                            "minimum": 0,
                            "description": "The face card",
                            "format": "int32"
                        },
                        "required": true
                    },
                    {
                        "name": "suit",
                        "in": "path",
                        "description": "The suit",
                        "schema": {
                            "$ref": "#/components/schemas/Suit"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "IsFaceCard 200 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/IsFaceCardResponseContent"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/playingCardDescription": {
            "post": {
                "description": "A description of a card from a stanard deck of playing cards",
                "operationId": "PlaceCardPicture",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/PlaceCardPictureRequestContent"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "PlaceCardPicture 200 response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/PlaceCardPictureResponseContent"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "IsFaceCardResponseContent": {
                "type": "object",
                "properties": {
                    "b": {
                        "type": "boolean"
                    }
                }
            },
            "PlaceCardPictureRequestContent": {
                "type": "object",
                "properties": {
                    "faceCard": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "suit": {
                        "$ref": "#/components/schemas/Suit"
                    }
                },
                "required": [
                    "faceCard",
                    "suit"
                ]
            },
            "PlaceCardPictureResponseContent": {
                "type": "object",
                "properties": {
                    "s": {
                        "type": "string"
                    }
                }
            },
            "Suit": {
                "type": "string",
                "enum": [
                    "diamond",
                    "club",
                    "heart",
                    "spade"
                ]
            }
        }
    }
}